{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Run Nuxt App with npm (Node 20.10)",
        "runtimeVersion": "20.10",
        "runtimeExecutable": "${env:HOME}/.nvm/versions/node/v20.10.0/bin/npm",
        "args": ["run", "dev"],
        "cwd": "${workspaceFolder}",
        "console": "integratedTerminal",
        "env": {
          "NODE_ENV": "development"
        }
      }
    ]
  }
  